[{{ USERNAME }}]

user = {{ USERNAME }}
group = {{ USERNAME }}

listen = 127.0.0.1:{{ FPM_POOL_TCP }}
listen.owner = {{ USERNAME }}
listen.group = {{ USERNAME }}

pm = dynamic
pm.max_children = 50
pm.start_servers = 20
pm.min_spare_servers = 10
pm.max_spare_servers = 30

php_admin_value[error_log] = {{ HOME_FOLDER }}/web/{{ DOMAIN }}/logs/fpm-php.{{ USERNAME }}.log
